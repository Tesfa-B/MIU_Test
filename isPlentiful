#include <stdio.h>

int main()
{
	int size;

	int array [] = {1,0,0,0,0};
	size = sizeof(arr[]) / sizeof (arr[0]);
	isPlentiful(arr, size);
	
return (0);
}
int isPlentiful(int arr[], int len)
{
	int i;
	int zeros = 0;
	
	for(i = 0; i < len; i++)
	{
		if (arr[i] == 0 && arr[i + 1] && arr [i + 2] && arr [i + 3] && arr [i + 4] == 0)
		{
			Return (1);
		}
	} 
	return (0);
}
